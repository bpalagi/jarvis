{
    "name": "jarvis",
    "productName": "Jarvis",
    "version": "0.2.4",
    "description": "AI Assistant",
    "main": "src/index.js",
    "scripts": {
        "setup": "npm install && cd jarvis_web && npm install && npm run build && cd .. && npm start",
        "start": "npm run build:renderer && electron .",
        "package": "npm run build:all && electron-builder --dir",
        "postinstall": "electron-builder install-app-deps",
        "build:renderer": "node build.js",
        "build:web": "cd jarvis_web && npm run build && cd ..",
        "build:all": "npm run build:renderer && npm run build:web",
        "package:linux": "npm run build:all && electron-builder --linux",
        "watch:renderer": "node build.js --watch"
    },
    "keywords": [
        "jarvis",
        "ai assistant",
        "real-time",
        "live summary",
        "contextual ai"
    ],
    "author": {
        "name": "TangLLC",
        "email": "contact@tangllc.com"
    },
    "license": "GPL-3.0",
    "dependencies": {
        "@deepgram/sdk": "^4.9.1",
        "@google/genai": "^1.8.0",
        "@google/generative-ai": "^0.24.1",
        "better-sqlite3": "^9.6.0",
        "cors": "^2.8.5",
        "dotenv": "^17.0.0",
        "electron-store": "^8.2.0",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "keytar": "^7.9.0",
        "node-fetch": "^2.7.0",
        "react-hot-toast": "^2.5.2",
        "sharp": "^0.34.2",
        "validator": "^13.11.0",
        "wait-on": "^8.0.3",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@electron/fuses": "^1.8.0",
        "electron": "^30.5.1",
        "electron-builder": "^26.0.12",
        "electron-reloader": "^1.2.3",
        "esbuild": "^0.25.5",
        "prettier": "^3.6.2"
    }
}
